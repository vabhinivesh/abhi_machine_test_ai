{
  "name": "cpq-agent",
  "version": "1.0.0",
  "description": "CPQ Agent for Industrial Pumps with Temporal Workflow",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/cli/interactive-quote.ts",
    "worker": "ts-node src/worker.ts",
    "test": "cross-env DEBUG= jest --silent",
    "test:watch": "jest --watch",
    "temporal": "docker-compose up -d",
    "temporal:stop": "docker-compose down",
    "temporal:logs": "docker-compose logs -f temporal",
    "dev": "concurrently \"npm run worker\" \"npm run start\""
  },
  "keywords": [
    "cpq",
    "temporal",
    "agent",
    "pumps"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@openai/agents": "^0.1.9",
    "@temporalio/client": "^1.8.0",
    "@temporalio/testing": "^1.8.0",
    "@temporalio/worker": "^1.8.0",
    "@temporalio/workflow": "^1.8.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "concurrently": "^8.2.0",
    "csv-parse": "^5.5.0",
    "dotenv": "^17.2.3",
    "inquirer": "^9.2.7",
    "nanoid": "^4.0.2",
    "ollama": "^0.5.0",
    "open": "^9.1.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "cross-env": "^10.1.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "collectCoverage": false,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.tsx?$": [
        "ts-jest",
        {
          "tsconfig": {
            "module": "commonjs",
            "esModuleInterop": true,
            "allowSyntheticDefaultImports": true
          }
        }
      ]
    },
    "silent": true,
    "verbose": false
  }
}
